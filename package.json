{
    "name": "@hckrnews/validator",
    "version": "8.0.3",
    "description": "Object validator",
    "main": "dist/validator.cjs",
    "type": "module",
    "files": [
        "src/validator.js",
        "dist/validator.cjs",
        "dist/validator.cjs.map",
        "dist/validator.js",
        "dist/validator.js.map",
        "dist/validator.module.mjs",
        "dist/validator.module.mjs.map",
        "dist/validator.umd.cjs",
        "dist/validator.umd.cjs.map"
    ],
    "source": "src/validator.js",
    "module": "dist/validator.module.mjs",
    "unpkg": "dist/validator.umd.cjs",
    "umd:main": "dist/validator.umd.cjs",
    "exports": {
        "node": {
            "default": "dist/validator.cjs",
            "module": "./src/validator.js",
            "require": "./dist/validator.cjs"
        },
        "require": "./dist/validator.cjs",
        "default": "./dist/validator.cjs"
    },
    "scripts": {
        "lint": "eslint src",
        "lint:report": "eslint src --config .eslintrc -f json -o report.json",
        "lint:fix": "eslint src --fix",
        "test": "c8 node --test tests/unit/validator.spec.js",
        "cpd": "node_modules/jscpd/bin/jscpd src",
        "vulnerabilities": "npm audit --production",
        "build": "microbundle --target node"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hckrnews/validator.git"
    },
    "author": {
        "name": "Pieter Wigboldus",
        "url": "https://hckr.news/"
    },
    "license": "AGPL-3.0",
    "bugs": {
        "url": "https://github.com/hckrnews/validator/issues"
    },
    "homepage": "https://github.com/hckrnews/validator#readme",
    "devDependencies": {
        "@hckrnews/eslint-config": "^3.0.0",
        "c8": "^7.11.3",
        "eslint": "^8.18.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-html": "^6.1.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.0",
        "eslint-plugin-prettier": "^4.1.0",
        "jscpd": "^3.3.25",
        "microbundle": "^0.15.1",
        "prettier": "^2.3.0"
    },
    "engines": {
        "node": ">= 18"
    },
    "keywords": [
        "objects",
        "validator"
    ],
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/w3nl"
    }
}
